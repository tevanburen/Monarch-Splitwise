{
	"name": "Monarch-Splitwise",
	"version": "1.0",
	"manifest_version": 3,
	"content_scripts": [
		{
			"js": ["dist/app.js"],
			"matches": ["*://app.monarch.com/*"]
		}
	],
	"web_accessible_resources": [
		{
			"resources": ["dist/page-context-injection.js", "dist/style.css"],
			"matches": ["*://app.monarch.com/*"]
		}
	]
}
