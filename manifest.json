{
	"name": "Monarch-Splitwise",
	"version": "1.0",
	"manifest_version": 3,
	"content_scripts": [
		{
			"js": ["dist/outer-root.js"],
			"matches": ["*://app.monarch.com/*", "*://secure.splitwise.com/*"]
		}
	],
	"web_accessible_resources": [
		{
			"resources": [
				"dist/page-context-injection.js",
				"dist/inner-root.html",
				"dist/inner-root.js",
				"dist/inner-root.css"
			],
			"matches": ["*://app.monarch.com/*", "*://secure.splitwise.com/*"]
		}
	]
}
